        "CenterPosition": "readonly",
        "Chicken": "readonly",
        "CinematicIntroLevel": "readonly",
        "ClickEvents": "readonly",
        "Clipboard": "readonly",
        "Collision": "readonly",
        "Compass": "readonly",
        "Controller": "readonly",
        "Conversation": "readonly",
        "Crate": "readonly",
